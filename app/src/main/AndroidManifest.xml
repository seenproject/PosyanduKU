<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="satella.app.posyanduku">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".posyandu.jadwal.KalenderJadwalActivity"></activity>
        <activity android:name=".posyandu.profil.ProfilPosyanduActivity" />
        <activity android:name=".posyandu.data.DataBayiActivity" />
        <activity android:name=".posyandu.timbang.TimbangActivity" />

        <activity android:name=".posyandu.PosyanduMainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".profile.data_profile" />
        <activity
            android:name=".data_pages.data_orang_tua.DataOrangTuaActivity"
            android:parentActivityName=".data_pages.DataBalitaActivity" />
        <activity
            android:name=".data_pages.data_anak.UpdateDeleteActivity"
            android:parentActivityName=".data_pages.daftar_keluarga.DaftarKeluargaActivity" />
        <activity
            android:name=".data_pages.daftar_keluarga.DaftarKeluargaActivity"
            android:parentActivityName=".data_pages.DataBalitaActivity" />
        <activity
            android:name=".data_pages.data_anak.InsertDataActivity"
            android:parentActivityName=".data_pages.DataBalitaActivity" />
        <activity
            android:name=".data_pages.DataBalitaActivity"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".Anak_Package.note.NoteActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>